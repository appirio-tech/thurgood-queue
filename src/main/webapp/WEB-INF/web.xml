<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    version="2.5">

  <welcome-file-list>
    <welcome-file>stats.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>setpSendersReceivers</servlet-name>
    <servlet-class>com.cloudspokes.squirrelforce.SetupSendersReceivers</servlet-class>
    <init-param>
    	<param-name>langs</param-name>
        <param-value>java;ruby;apex;javascript;python</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>setpSendersReceivers</servlet-name>
    <url-pattern>/setpSendersReceivers</url-pattern>
  </servlet-mapping>
</web-app>
